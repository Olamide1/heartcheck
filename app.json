{
  "expo": {
    "name": "HeartCheck",
    "slug": "heartcheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8FA68E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.heartcheck.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8FA68E"
      },
      "package": "com.heartcheck.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "heartcheck"
  }
}
